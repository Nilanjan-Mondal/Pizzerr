{
    "version": 2,
    "functions": {
      "src/index.js": {
        "maxDuration": 30
      }
    },
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }
  